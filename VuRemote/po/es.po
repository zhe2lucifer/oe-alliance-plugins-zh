#, python-format
msgid " in %d seconds."
msgstr " en %d segundos."

msgid "Cancel"
msgstr "Cancelar"

msgid "FILE NOT EXIST : /proc/stb/fp/remote_code"
msgstr "El archivo /proc/stb/fp/remote_code no existe"

